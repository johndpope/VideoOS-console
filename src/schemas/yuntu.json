{
  "title": "",
  "type": "object",
  "required": [
    "creativeName",
    "interactionTypeId",
    "interactionTemplateId",
    "imageUrl",
    "positionX",
    "positionY"
  ],
  "properties": {
    "creativeName": {
      "title": "素材名称",
      "type": "string",
      "maxLength": 10,
      "pattern": "[0-9A-Za-z\u4e00-\u9fa5-]+$"
    },
    "interactionTypeId": {
      "title": "素材类型",
      "type": "integer",
      "default": "请选择",
      "enum": [],
      "enumNames": []
    },
    "interactionTemplateId": {
      "title": "素材模板",
      "type": "integer",
      "enum": [],
      "enumNames": []
    },
    "imageUrl": {
      "type": "string",
      "title": "图片"
    },
    "isShowAds": {
      "type": "boolean",
      "title": "广告标识是否可见",
      "default": true
    },
    "isShowClose": {
      "type": "boolean",
      "title": "关闭按钮是否可见",
      "default": true
    },
    "needShowOnPortrait": {
      "type": "boolean",
      "title": "竖屏是否可见",
      "default": false
    },
    "positionX": {
      "type": "number",
      "title": "横屏场景，广告图片距屏幕左上角的距离占屏幕宽度比(positionX)",
      "default": 0.5
    },
    "positionY": {
      "type": "number",
      "title": "横屏场景，广告图片距屏幕左上角的距离占屏幕高度比(positionY)",
      "default": 0.5
    },
    "monitorLinks": {
      "type": "array",
      "title": "点击监控链接",
      "items": {
        "type": "string",
        "format": "uri"
      }
    },
    "exposureLinks": {
      "type": "array",
      "title": "曝光监控链接",
      "items": {
        "type": "string",
        "format": "uri"
      }
    },
    "linkUrl": {
      "type": "string",
      "format": "uri",
      "title": "跳转外链链接"
    }
  }
}
