{
    "title": "",
    "definitions": {
      "InfoItem": {
        "type": "object",
        "properties": {
          "name": {
            "title": "投票对象名称",  
            "type": "string"  
          },
          "vote_img": {
            "title": "投票图片",  
            "type": "string", 
            "format": "data-url" 
          }
        }  
      }  
    },
    "type": "object",
    "required": ["popular_point_title", "avatar"],
    "properties": {
      "material_name": {
        "title": "素材名称",  
        "type": "string"  
      },
      "material_type": {
        "title": "素材类型",  
        "type": "string",
        "default": "请选择",
        "enum": [
          "云图",
          "中插",
          "红包",
          "投票"
        ],
        "enumNames": [
          "云图",
          "中插",
          "红包",
          "投票"  
        ]  
      },
      "material_model": {
        "title": "素材模板",
        "type": "string",
        "enum": [
          "请选择"  
        ],
        "enumNames": [
          "请选择"  
        ]
      },
      "popular_point_title": {
        "type": "string",
        "title": "热点标题",
        "default": ""  
      },
      "avatar": {
        "type": "string",
        "format": "data-url",
        "title": "热点图片"
      },
      "countdown_duration": {
        "type": "string",
        "title": "倒计时持续时间",
        "enum": [
          "10s",
          "15s",
          "20s",
          "25s",
          "30s",
          "自定义"  
        ],
        "enumNames": [
          "10秒",
          "15秒",
          "20秒",
          "25秒",
          "30秒",
          "自定义"
        ]
      },
      "show_ad_sign": {
        "type": "boolean",
        "title": "广告标识是否可见"  
      },
      "infoItems": {
        "type": "array",
        "title": "信息选项",
        "minItems": 1,
        "items": {
          "$ref": "#/definitions/InfoItem"  
        }  
      },
      "exposureLinks": {
        "type": "array",
        "title": "热点曝光监控链接",
        "minItems": 1,
        "items": {
          "type": "string"  
        }  
      },
      "rules": {
        "type": "string",
        "title": "投票规则",
        "default": ""  
      }
    }  
  }